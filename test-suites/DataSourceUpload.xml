<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestSuite">
	<groups>
		<run>
			<include name="DataSourceUpload"/>
		</run>
	</groups>
	
  <test thread-count="5" name="DataSource Upload">
    <classes>
      <class name="com.Test.VerifyDataSourceUpload"/>
      </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="DataSource Comparision">
    <classes>
      <class name="com.Test.VerifyDataComparision"/>
      </classes>
  </test> <!-- Test -->
    
</suite> <!-- Suite -->
